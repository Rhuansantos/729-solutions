extends layout/main
block content
  main
    #carouselExampleControls.carousel.slide(data-ride='carousel')
    .carousel-inner
      .carousel-item.active
        img.d-block.w-100(src='https://source.unsplash.com/user/willianjusten/1920x500', alt='First slide')
      .carousel-item
        img.d-block.w-100(src='https://source.unsplash.com/user/willianjusten/1920x500', alt='Second slide')
      .carousel-item
        img.d-block.w-100(src='https://source.unsplash.com/user/willianjusten/1920x500', alt='Third slide')
    a.carousel-control-prev(href='#carouselExampleControls', role='button', data-slide='prev')
      span.carousel-control-prev-icon(aria-hidden='true')
      span.sr-only Previous
    a.carousel-control-next(href='#carouselExampleControls', role='button', data-slide='next')
      span.carousel-control-next-icon(aria-hidden='true')
      span.sr-only Next

    section.container
      ul
        li 1
        li 2
        li 3